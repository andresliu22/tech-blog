{{#each posts}}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
        <a href="/post/{{id}}">{{title}}</a>
    </h2>
    <p>Created by {{user.fist_name user.last_name}} on {{format_date date_created}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{content}}
    </p>
  </div>
</div>
{{/each}}
